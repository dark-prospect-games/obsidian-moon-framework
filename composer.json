{
    "name": "dark-prospect-games/obsidian-moon-framework",
    "description": "An Open Source, Lightweight and 100% Modular Framework in PHP",
    "version": "1.0.0",
    "type": "project",
    "keywords": [
        "framework"
    ],
    "homepage": "https://github.com/dark-prospect-games/obsidian-moon-engine",
    "license": "MIT",
    "authors": [
        {
            "name": "Alfonso E Martinez, III",
            "email": "admin@darkprospect.net",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": ">7.2.0",
        "dark-prospect-games/obsidian-moon-engine": "dev-master"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "preferred-install": {
            "roave/security-advisories": "source",
            "*": "dist"
        }
    },
    "minimum-stability": "stable"
}
